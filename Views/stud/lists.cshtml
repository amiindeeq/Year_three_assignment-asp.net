<h3>Student Lists</h3>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Mobile</th>
            <th>Course</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Mobile</td>
                <td>@student.Course</td>
            </tr>
        }
    </tbody>
</table>
<a class="btn btn-primary" asp-controller="stud" asp-action="assignment">Go Back</a>